{{!-- <div>{{statusIcon status}} {{name}} {{statusBar this}}</div> --}}
<div class="summary-module">
	<div class="name">{{name}}</div>
	<div id="overal" class="graph round-corner-all">
		<div class="rls-passed even"><div class="te">Passed</div><span class="value label label-success">{{passed}}</span></div>
		<div class="rls-failed odd"><div class="te">Failed</div><span class="value label label-danger">{{failed}}</span></div>
		<div class="rls-blocked even"><div class="te">Blocked</div><span class="value label label-warning">{{blocked}}</span></div>
		<div class="rls-inp odd"><div class="te">In Progress</div><span class="value label label-info">{{inprogress}}</span></div>
		<div class="rls-nrun even"><div class="te">Not Run</div><span class="value label">{{notrun}}</span></div>
		<div class="rls-total odd"><div class="te">Total</div><span class="value label label-inverse">{{total}}</span></div>
	</div>
</div>